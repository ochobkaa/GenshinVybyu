{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "GenshinVybyu": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WebhookConfiguration": {
    "HostAddress": "http://bot:8080",
    "UseNgrok": true,
    "Route": "/bot",
    "RequestRetryDelay": 10000,
    "RequestRetryTimes": 3
  },
  "BotConfiguration": {
    "ModelDataPath": "ModelData",
    "ModelDataFilenameFormat": "model_{fiftyfifty}_c{cchar}_r{rweapon}",
    "ExecutorRetryDelay": 2000,
    "ExecutorRetryTimes": 5,
    "RollPrimogemsCost": 160,
    "CharPity": 90,
    "WeaponPity": 80,
    "CommandPrefix": "/",
    "KeyAttrValuePrefix": "=",
    "SplashProb": 0.33
  }
}
